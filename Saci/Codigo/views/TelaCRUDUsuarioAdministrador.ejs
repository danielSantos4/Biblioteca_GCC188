<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
    </head>
    <body>
        <div class="">
            <h2>Tabela de todos os usu√°rios</h2>
                <table border="1">
                    <thead>
                        <tr>
                            <th>Nome</th>
                            <th>Email</th>
                            <th>Data de nascimento</th>
                        </tr>
                    </thead>
                    
                    <tbody>
                        <% table.forEach((row) => { %>
                            <tr>
                              <td><%= row.nome %></td>
                              <td><%= row.email %></td>
                              <td><%= row.data_nascimento %></td>
                              <td>
                                  <a href="http://localhost:3000/user/delete/<%=row.email%>">Delete</a>
                              </td>
                            </tr>
                        <% }); %>
                    </tbody>
                </table>
                </div>
    </body>
</html>